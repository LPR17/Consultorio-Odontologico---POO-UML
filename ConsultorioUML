'Trabajo Practico N°3 - UML - Consultorio Odontológico

@startuml

title Consultorio Odontologico

class Consultorio{

}

abstract class Persona{
    #nombre: String
    #apellido: String
    #dni: int
    #edad: int
    --

}

class Paciente{
    -obraSocial: String
    --
    +solicitarTurno:
    +consultarHistorialMedico:
}

class Odontologo{
    -especialidad: String
    -matricula: int
    --

}

class Administrativo{

}


class HistorialClinico{

}



'Relacion de herencia
Persona <|-- Paciente: es un
Persona <|-- Odontologo: es un
Persona <|-- Administrativo: es un

'Relación de composición
Paciente *-- HistorialClinico: tiene un

'Relación de agregación
Consultorio o-- Odontologo: emplea
Consultorio o-- Administrativo: emplea

@enduml